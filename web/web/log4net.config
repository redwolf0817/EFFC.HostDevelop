<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <log4net>
    <root>
      <!--<level value="ALL" />
      <appender-ref ref="rollingFile" />-->
    </root>

    <logger name="WarnLog">
      <level value="Warn" />
      <appender-ref ref="warnFile" />
    </logger>
    <logger name="DebugLog">
      <level value="Debug" />
      <appender-ref ref="debugFile" />
    </logger>
    <logger name="ErrorLog">
      <level value="Error" />
      <appender-ref ref="errorFile" />
    </logger>
    <logger name="InfoLog">
      <level value="Info" />
      <appender-ref ref="infoFile" />
    </logger>
    <logger name="FatalLog">
      <level value="Fatal" />
      <appender-ref ref="fatalFile" />
    </logger>
    
    <appender name="warnFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File"  value="C://log//effcdev//Warn-" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%t]:%m" />
      </layout>
    </appender>
    <appender name="debugFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File"  value="C://log//effcdev//Debug-" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%t]:%m" />
      </layout>
    </appender>
    <appender name="errorFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File"  value="C://log//effcdev//Error-" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%t]:%m" />
      </layout>
    </appender>
    <appender name="infoFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File"  value="C://log//effcdev//Info-" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%t]:%m" />
      </layout>
    </appender>
    <appender name="fatalFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File"  value="C://log//effcdev//Fatal-" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%t]:%m" />
      </layout>
    </appender>
    <appender name="rollingFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File"  value="C://log//effcdev//" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%t]:%m" />
      </layout>
    </appender>
    <appender name="consoleprint" type="log4net.Appender.ConsoleAppender" >
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%n%d [%thread]:%m" />
        <!-- <ConversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />-->
      </layout>
    </appender>

   

  </log4net>
</configuration>
